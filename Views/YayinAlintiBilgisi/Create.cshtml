@model TaramaMVC.Models.YayinAlintiBilgisi

@{
    ViewData["Title"] = "YAYIN ALINTI BİLGİSİ";
}
<div class="container-sm">
<h1 class="bg-info text-white">YAYIN ALINTI BİLGİSİ EKLE</h1>
 
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="personelYayinBilgileriId" class="control-label"></label>
                <input asp-for="personelYayinBilgileriId" class="form-control" />
                <span asp-validation-for="personelYayinBilgileriId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Tip" class="control-label"></label>
                <input asp-for="Tip" class="form-control" />
                <span asp-validation-for="Tip" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Ad" class="control-label"></label>
                <input asp-for="Ad" class="form-control" />
                <span asp-validation-for="Ad" class="text-danger"></span>
            </div>
         @*   <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>*@

                <div class="Row md-2 mt-2">
                    <button type="submit" class="form-control btn-secondary">
                        <svg class="bi d-block mx-auto mb-1" width="16" height="16"><use xlink:href="#save1" /></svg>
                        Kaydet
                    </button>
                </div>
        </form>
    </div>
</div>

    <div class="Row md-2 mt-2">
        <a asp-action="Index" class="form-control btn btn-secondary">
            <svg class="bi d-block mx-auto mb-1" width="16" height="16"><use xlink:href="#back1" /></svg>
            Geri
        </a>
    </div>
</div>
@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
